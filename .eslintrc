{
  "parser": "babel-eslint",
  "extends": ["react-app", "eslint:recommended", "plugin:react/recommended", "plugin:import/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "import"],
  "settings": {
    "react": {
      "version": "16.6.0"
    }
  },
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn",
    "react/display-name": "off",
    "import/no-unresolved": ["error", { "ignore": ["^@/", "Actions", "Assets", "Components", "Pages"] }]
  }
}
